{
  "compilerOptions": {
    "sourceMap": true,
    "suppressImplicitAnyIndexErrors": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "target": "es2018",
    "outDir": "dist",
    "lib": [
      "es2018"
    ]
  }
}